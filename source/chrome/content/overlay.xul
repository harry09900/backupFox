<overlay id="backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66-overlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
	
	<script src="UTF8Coder.js"/>
	<script src="overlay.js"/>
	
	<stringbundleset id="stringbundleset">
		<stringbundle id="backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66-strings" src="chrome://backupfox/locale/overlay.properties"/>
	</stringbundleset>
	
	<!-- bookmarks menu add-on -->
	<menupopup id="appmenu_bookmarksPopup">
		<menuitem id="backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66-bookmarksMenuRunItemA" class="menuitem-iconic"
			insertafter="appmenu_showAllBookmarks" label="&bookmarksMenuItem.label;" tooltiptext="&bookmarksMenuItem.tooltip;"
			image="chrome://backupfox/skin/iconA.png" oncommand="window.backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66_overlay.MenuBookmarksBackup();" />
	</menupopup>
	<menupopup id="bookmarksMenuPopup">
		<menuitem id="backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66-bookmarksMenuRunItemB" class="menuitem-iconic"
			insertafter="bookmarksShowAll" label="&bookmarksMenuItem.label;" tooltiptext="&bookmarksMenuItem.tooltip;"
			image="chrome://backupfox/skin/iconA.png" oncommand="window.backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66_overlay.MenuBookmarksBackup();" />
	</menupopup>
	
	<!-- history menu add-on -->
	<menupopup id="appmenu_historyMenupopup">
		<menuitem id="backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66-historyMenuRunItemA" class="menuitem-iconic"
			insertafter="appmenu_showAllHistory" label="&historyMenuItem.label;" tooltiptext="&historyMenuItem.tooltip;"
			image="chrome://backupfox/skin/iconA.png" oncommand="window.backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66_overlay.MenuHistoryBackup();" />
	</menupopup>
	<menupopup id="goPopup">
		<menuitem id="backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66-historyMenuRunItemB" class="menuitem-iconic"
			insertafter="menu_showAllHistory" label="&historyMenuItem.label;" tooltiptext="&historyMenuItem.tooltip;"
			image="chrome://backupfox/skin/iconA.png" oncommand="window.backupfox_959a5970_ada3_11e0_9f1c_0800200c9a66_overlay.MenuHistoryBackup();" />
	</menupopup>
	
</overlay>

